ata","data":{"text/plain":["<IPython.core.display.Javascript object>"],"application/javascript":["download(\"download_d0b51074-b7d2-4e81-860b-4c47dc49f486\", \"final_submission.csv\", 184576)"]},"metadata":{}}]}]}